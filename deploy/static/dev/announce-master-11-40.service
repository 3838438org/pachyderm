[Unit]
Description = pfs announce: master
PartOf = master-11-40.service
BindsTo = master-11-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/11-40 '%H:60582' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/11-40

[X-Fleet]
MachineOf = master-11-40.service
