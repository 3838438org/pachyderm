[Unit]
Description = pfs announce: master
PartOf = master-33-40.service
BindsTo = master-33-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/33-40 '%H:65201' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/33-40

[X-Fleet]
MachineOf = master-33-40.service
