[Unit]
Description = pfs announce: master
PartOf = master-30-40.service
BindsTo = master-30-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/30-40 '%H:63662' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/30-40

[X-Fleet]
MachineOf = master-30-40.service
