[Unit]
Description = pfs announce: master
PartOf = master-12-40.service
BindsTo = master-12-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/12-40 '%H:58989' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/12-40

[X-Fleet]
MachineOf = master-12-40.service
