[Unit]
Description = pfs announce: master
PartOf = master-16-40.service
BindsTo = master-16-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/16-40 '%H:52733' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/16-40

[X-Fleet]
MachineOf = master-16-40.service
