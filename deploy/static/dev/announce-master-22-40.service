[Unit]
Description = pfs announce: master
PartOf = master-22-40.service
BindsTo = master-22-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/22-40 '%H:59104' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/22-40

[X-Fleet]
MachineOf = master-22-40.service
