[Unit]
Description = pfs announce: master
PartOf = master-31-40.service
BindsTo = master-31-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/31-40 '%H:63023' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/31-40

[X-Fleet]
MachineOf = master-31-40.service
