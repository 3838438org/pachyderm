[Unit]
Description = pfs announce: master
PartOf = master-24-40.service
BindsTo = master-24-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/24-40 '%H:49710' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/24-40

[X-Fleet]
MachineOf = master-24-40.service
