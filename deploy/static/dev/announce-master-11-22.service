[Unit]
Description = pfs announce: master
PartOf = master-11-22.service
BindsTo = master-11-22.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/11-22 '%H:59267' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/11-22

[X-Fleet]
MachineOf = master-11-22.service
