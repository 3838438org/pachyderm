[Unit]
Description = pfs announce: master
PartOf = master-1-12.service
BindsTo = master-1-12.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/1-12 '%H:51160' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/1-12

[X-Fleet]
MachineOf = master-1-12.service
