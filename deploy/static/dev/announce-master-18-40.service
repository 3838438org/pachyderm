[Unit]
Description = pfs announce: master
PartOf = master-18-40.service
BindsTo = master-18-40.service

[Service]
ExecStart = /bin/sh -c "while true; do etcdctl set /pfs/master/18-40 '%H:55113' --ttl 60;sleep 45;done"
ExecStop = /usr/bin/etcdctl rm /pfs/master/18-40

[X-Fleet]
MachineOf = master-18-40.service
